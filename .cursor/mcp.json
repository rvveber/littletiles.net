{
    "mcpServers": {
        "dbhub": {
            "command": "bunx @bytebase/dbhub",
            "env": {
                "TRANSPORT": "stdio",
                "DSN": "postgres://db-user:db-password@localhost:5432/db-name?sslmode=disable",
                "READONLY": "true"
            }
        }
    }
}